package core;

import java.time.LocalDate;

public class Grocery {
	String Name;
	double PricePerUnit;
	int Quantity;
	LocalDate LastUpdated;
	
	public String getName() {
		return Name;
	}
	public double getPricePerUnit() {
		return PricePerUnit;
	}
	public int getQuantity() {
		return Quantity;
	}
	public LocalDate getLastUpdated() {
		return LastUpdated;
	}

}
